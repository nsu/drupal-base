{
    "apache": {
        "package": "apache2",
        "binary": "/usr/sbin/httpd2",
        "apachectl": "/usr/sbin/apache2ctl",
        "dir": "/etc/apache2",
        "lib_dir": "/usr/lib64/apache2",
        "libexec_dir": "/usr/lib64/apache2",
        "cache_dir": "/var/cache/apache2",
        "cgibin_dir": "/srv/www/cgi-bin",
        "docroot_dir": "/srv/www/htdocs",
        "conf": "/etc/apache2/httpd.conf",
        "perl_pkg": "perl",
        "log_dir": "/var/log/apache2",
        "root_group": "root",
        "service_name": "apache2",
        "service_restart_command": "/sbin/service apache2 restart && sleep 1",
        "service_reload_command":  "/sbin/service apache2 reload && sleep 1",
        "default_site_name": "000-default",
        "default_site_enabled": false,
        "default_modules": [
            "status",
            "alias",
            "auth_basic",
            "authn_file",
            "authz_core",
            "authz_groupfile",
            "authz_host",
            "authz_user",
            "autoindex",
            "dir",
            "env",
            "mime",
            "negotiation",
            "setenvif"
        ]
    }
}
